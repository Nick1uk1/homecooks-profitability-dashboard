# HomeCooks Profitability Dashboard Configuration
# Copy this file to .env and fill in your values

# Shopify API Configuration
SHOPIFY_STORE_DOMAIN=your-store.myshopify.com
SHOPIFY_ACCESS_TOKEN=shpat_xxxxxxxxxxxxxxxxxxxx
SHOPIFY_API_VERSION=2024-07

# Linnworks API Configuration (for dispatch dates and retail orders)
LINNWORKS_APP_ID=your-app-id
LINNWORKS_APP_SECRET=your-app-secret
LINNWORKS_INSTALL_TOKEN=your-install-token

# Appstle Subscriptions API Configuration
APPSTLE_API_KEY=your-appstle-api-key
